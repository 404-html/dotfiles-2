<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE KSysGuardWorkSheet>
<WorkSheet title="Detailed" interval="0.5" columns="2" locked="0" rows="4">
 <host name="localhost" shell="" port="-1" command="ksysguardd"/>
 <display class="FancyPlotter" vScroll="0" title="Total Load" columnSpan="1" labels="1" hScale="6" autoRange="1" rowSpan="1" version="1" manualRange="0" fontSize="8" vDistance="30" unit="" showUnit="0" hLines="1" stacked="0" svgBackground="" row="0" column="0" vLines="0">
  <beam sensorType="float" sensorName="cpu/cpu0/TotalLoad" hostName="localhost" color="0xff0057ae"/>
  <beam sensorType="float" sensorName="cpu/cpu1/TotalLoad" hostName="localhost" color="0xffe20800"/>
  <beam sensorType="float" sensorName="cpu/cpu2/TotalLoad" hostName="localhost" color="0xfff3c300"/>
  <beam sensorType="float" sensorName="cpu/cpu3/TotalLoad" hostName="localhost" color="0xffff0000"/>
 </display>
 <display class="FancyPlotter" vScroll="0" title="Fan" columnSpan="1" labels="1" hScale="6" autoRange="1" rowSpan="1" version="1" manualRange="0" fontSize="8" vDistance="30" unit="" showUnit="0" hLines="1" stacked="0" svgBackground="" row="1" column="0" vLines="0">
