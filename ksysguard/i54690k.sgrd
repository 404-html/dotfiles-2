  <beam sensorType="float" hostName="localhost" color="0xff00aa00" summationName="RX" regexpSensorName="network/interfaces/(?!lo|bridge|usbus|bond).*/receiver/data"/>
  <beam sensorType="float" hostName="localhost" color="0xffff0000" summationName="TX" regexpSensorName="network/interfaces/(?!lo|bridge|usbus|bond).*/transmitter/data"/>
