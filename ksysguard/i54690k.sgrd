  <beam sensorType="float" hostName="localhost" color="0xff0057ae" summationName="Reads" regexpSensorName="disk/sd[a-z]*_.*/Rate/rblk"/>
  <beam sensorType="float" hostName="localhost" color="0xffe20800" summationName="Writes" regexpSensorName="disk/sd[a-z]*_.*/Rate/wblk"/>
